<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bf.esiupb.controleur.principaleControleur" >
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="Fichier" fx:id="menuprincipal">
                    <items>
                        <MenuItem fx:id="menuenregistrer" mnemonicParsing="false" text="Enregistrer" onAction="#afficherEnregistrer" />
                        <MenuItem mnemonicParsing="false" text="Lister" onAction="#afficherLister"/>
                        <MenuItem mnemonicParsing="false" text="Quitter" onAction="#quitterApplication"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" fx:id="paneprincipal"/>
    </children>
</VBox>
